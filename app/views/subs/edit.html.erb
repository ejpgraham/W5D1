<h1>Edit Sub!!!</h1>

<form class="" action="<%= sub_url(@sub) %>" method="post">
  <%= auth_token %>
  <input type="hidden" name="_method" value="patch">

  <label for="">Title
    <input type="text" name="sub[title]" value="<%= @sub.title %>">
  </label>

  <label for="">Description
    <input type="text" name="sub[description]" value="<%= @sub.description %>">
  </label>
  <input type="submit" name="" value="Edit Sub">
</form>
